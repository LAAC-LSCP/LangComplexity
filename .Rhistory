all_data$Age3=all_data$Age^3 #generate cubic component
all_data<-subset(all_data, !is.na(C_count)) #exclude N/A
View(all_data)
# add more information
all_data$coding<-ifelse(all_data$corpus %in% c("Solomon","French"),"lab","citsci")
