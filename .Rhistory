table(all_data$corpus)
table(all_data$Language) #shows N kids per language
table(all_data$C_count,all_data$Language)  #notice that all the moderate data comes from Tsimane
#histograms
hist(all_data$CR,main="CR",xlab="CR") #quite normally distributed
hist(all_data$CR[all_data$C_count=="Low"],main="Low Syllable Complexity",xlab="CR") #looks ok
hist(all_data$CR[all_data$C_count=="Moderate"],main="Moderate Syllable Complexity",xlab="CR")  #less good, quite flat
hist(all_data$CR[all_data$C_count=="High"],main="High Syllable Complexity",xlab="CR") #idem
